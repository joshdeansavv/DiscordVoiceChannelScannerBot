# Discord Bot Configuration
# Copy this file to .env and fill in your values

# Your Discord bot token (required)
DISCORD_TOKEN=your_bot_token_here

# Your Discord server (guild) ID (required)
DISCORD_GUILD_ID=your_guild_id_here

# Your Discord voice channel ID (required)
DISCORD_VOICE_CHANNEL_ID=your_voice_channel_id_here

# Stream URL (optional - defaults to JunctionNow radio)
STREAM_URL=https://audio.junctionnow.com:8000/radio.mp3 